<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item>
        <shape android:shape="rectangle">
            <solid android:color="#EEC8A7" />
            <corners android:radius="40dp" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="rectangle">
            <gradient
                android:angle="180"
                android:endColor="#FFD117"
                android:startColor="#FFDD39"
                android:type="linear"
                android:useLevel="true" />
                <stroke android:color="#222222" android:width="0.5dp"/>
            <corners android:radius="40dp" />
            >
        </shape>
    </item>

</layer-list>